# Copy this file to .env and fill in real values before running the bot.
# The Settings class in src/core/config.py loads ./ .env by default.

DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/discord_agent
DISCORD_TOKEN=your-discord-bot-token

# Google Gemini credentials
GEMINI_API_KEY=your-gemini-api-key
GOOGLE_CLIENT_SECRET=your-google-programmable-search-engine-id

# LangSmith observability (optional)
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your-langsmith-api-key
LANGSMITH_PROJECT=discord-agentic-bot

# Logging verbosity override (optional)
LOG_LEVEL=DEBUG
